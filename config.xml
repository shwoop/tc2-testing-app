<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tutorcruncher.mobile" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TutorCruncher</name>
    <description>
        Agency Management Software
    </description>
    <author email="info@tutorcruncher.com" href="http://www.tutorcruncher.com/">
        TutorCruncher
    </author>
    <access origin="https://secure.tutorcruncher.com"/>
    <access origin="https://tutorcruncher.s3.amazonaws.com"/>
    <access origin="https://tutorcruncher-public.s3.amazonaws.com"/>
    <content src="index.html" />
    <allow-intent href="https://*/*" />
